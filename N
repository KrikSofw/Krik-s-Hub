      elseif "InnerPeace Island" == Option then 
         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(144.021057, 87159.4844, 88.9619141, 0.342042685, -0, -0.939684391, 0, 1, -0, 0.939684391, 0, 0.342042685)
      elseif "BlazingVortex Island" == Option then 
         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(144.021057, 91354.4844, 88.9619141, 0.342042685, -0, -0.939684391, 0, 1, -0, 0.939684391, 0, 0.342042685)
   end  
   end,
})





