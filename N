 function skills()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(88, 4, 102)
 end

 function shop()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(125, 5, 29)
 end

 function kill ()
    Rayfield:Destroy()
 end

