spawn(function ()
    
while CoinBoost == true do
    
    local args = {
        [1] = "CoinBoost",
        [2] = 950000,
        [3] = 100000
    }
    
    game:GetService("ReplicatedStorage").Remotes.generateBoost:FireServer(unpack(args))
wait()    

end

end)

