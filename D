 local Dropdown = Tab:CreateDropdown({
    Name = "Areas",
    Options = {"RaceTrack","Trucks","Farm","Dock","SpaceStation","Emoji","Character","Nintendo","Rubik's Cube","Houses","MilteryBase","Pyramid","Castle","City","Towers","Volcano"},
